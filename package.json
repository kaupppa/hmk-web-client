{
  "name": "hmk-web-client",
  "version": "1.0.0",
  "description": "Web client based on Angular 2 and Webpack",
  "author": "Pauli Kauppinen",
  "license": "MIT",
  "homepage": "https://github.com/kaupppa/hmk-web-client",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "clean": "npm cache clean && npm run rimraf -- node_modules coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors ",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch  --content-base src/",
    "server:prod": "http-server dist --cors",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "start": "npm run server:dev",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "^9.1.2",
    "@angular/cli": "^9.0.6",
    "@angular/common": "9.0.6",
    "@angular/compiler-cli": "^9.0.6",
    "@angular/core": "9.0.6",
    "@angular/forms": "9.0.6",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "codelyzer": "^5.2.1",
    "core-js": "^3.6.4",
    "hammerjs": "^2.0.8",
    "ie-shim": "^0.1.0",
    "leaflet": "^1.6.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/compiler": "^9.0.6",
    "@types/chai": "^4.2.10",
    "@types/core-js": "^2.5.3",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "^13.9.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "es6-promise-loader": "^1.0.2",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "parse5": "^5.1.1",
    "raw-loader": "4.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "to-string-loader": "^1.1.6",
    "ts-helpers": "1.1.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "tslint-loader": "^3.5.4",
    "typescript": "~3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-md5-hash": "^0.0.6",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaupppa/hmk-web-client.git"
  }
}
